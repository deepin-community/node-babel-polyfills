{
  "arrowParens": "avoid",
  "trailingComma": "es5",
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "tabWidth": 2,
  "printWidth": 80,
  "overrides": [
    {
      "files": ["**/packages/*/src/**/*.{js,ts}", "**/packages/*/test/**/*.{js,ts}"],
      "parser": "babylon",
      "options": {
        "trailingComma": "all"
      }
    }
  ]
}
